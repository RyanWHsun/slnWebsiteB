@model IEnumerable<prjWebsiteB.Models.TPost>

@foreach (var item in Model)
{
    <div class="container-fluid pt-5 pb-3">
        <div class="row px-xl-5">
            <div class="col-lg-3 col-md-4 col-sm-6 pb-1" >
                <div class="product-item bg-light mb-4">
                    <div class="product-img position-relative overflow-hidden">
                        <partial name="_ShowPicture2Partial" model="@item" />
                        <div class="product-action">
                            <a class="btn btn-outline-dark btn-square" href="" onclick="btnDetails(event,@item.FPostId)" data-bs-toggle="modal" data-bs-target="#editPostModal"><i class="fa fa-search"></i></a>
                            <a class="btn btn-outline-dark btn-square" href="" onclick="btnEditByUser(event,@item.FPostId)" data-bs-toggle="modal" data-bs-target="#editPostModal"><i class="far fa-edit"></i></a>
                            <a class="btn btn-outline-dark btn-square" href="" onclick="btnDelete(event,@item.FPostId)" data-bs-toggle="modal" data-bs-target="#editPostModal"><i class="fas fa-trash-alt"></i></a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
}